#include <iostream>
using namespace std;

int main() {
int arr[5];
int reverse_array[5];

cout<<"The numbers of array are:"<<endl;

 for(int i=0; i<5; i++){
    cin>>arr[i];
 }

 for(int n=0; n<5; n++){
    cout<<arr[n]<<" ";
 }

 for(int i=4;i>=0;i--){
    cout<<arr[i]<<" ";
 }

return 0;
}

//Output:
//The numbers of array are:
//2 4 6 8 10
//2 4 6 8 10 10 8 6 4 2 
